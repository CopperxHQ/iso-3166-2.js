{
  "name": "@koshmoney/countries",
  "version": "1.0.1-beta.3",
  "description": "Complete ISO 3166-1 (countries) and ISO 3166-2 (subdivisions) lookup library with full TypeScript support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "country": ["./dist/country/index.d.ts"],
      "subdivision": ["./dist/subdivision/index.d.ts"],
      "subdivision/*": ["./dist/subdivision/data/*.d.ts"],
      "postalCode": ["./dist/postalCode/index.d.ts"],
      "currency": ["./dist/currency/index.d.ts"],
      "dialCode": ["./dist/dialCode/index.d.ts"],
      "geography": ["./dist/geography/index.d.ts"],
      "membership": ["./dist/membership/index.d.ts"]
    }
  },
  "sideEffects": [
    "./dist/subdivision/data/*.js",
    "./dist/subdivision/data/*.cjs",
    "./src/subdivision/data/*.ts"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./country": {
      "import": {
        "types": "./dist/country/index.d.ts",
        "default": "./dist/country/index.js"
      },
      "require": {
        "types": "./dist/country/index.d.cts",
        "default": "./dist/country/index.cjs"
      }
    },
    "./subdivision": {
      "import": {
        "types": "./dist/subdivision/index.d.ts",
        "default": "./dist/subdivision/index.js"
      },
      "require": {
        "types": "./dist/subdivision/index.d.cts",
        "default": "./dist/subdivision/index.cjs"
      }
    },
    "./subdivision/*": {
      "import": {
        "types": "./dist/subdivision/data/*.d.ts",
        "default": "./dist/subdivision/data/*.js"
      },
      "require": {
        "types": "./dist/subdivision/data/*.d.cts",
        "default": "./dist/subdivision/data/*.cjs"
      }
    },
    "./postalCode": {
      "import": {
        "types": "./dist/postalCode/index.d.ts",
        "default": "./dist/postalCode/index.js"
      },
      "require": {
        "types": "./dist/postalCode/index.d.cts",
        "default": "./dist/postalCode/index.cjs"
      }
    },
    "./currency": {
      "import": {
        "types": "./dist/currency/index.d.ts",
        "default": "./dist/currency/index.js"
      },
      "require": {
        "types": "./dist/currency/index.d.cts",
        "default": "./dist/currency/index.cjs"
      }
    },
    "./dialCode": {
      "import": {
        "types": "./dist/dialCode/index.d.ts",
        "default": "./dist/dialCode/index.js"
      },
      "require": {
        "types": "./dist/dialCode/index.d.cts",
        "default": "./dist/dialCode/index.cjs"
      }
    },
    "./geography": {
      "import": {
        "types": "./dist/geography/index.d.ts",
        "default": "./dist/geography/index.js"
      },
      "require": {
        "types": "./dist/geography/index.d.cts",
        "default": "./dist/geography/index.cjs"
      }
    },
    "./membership": {
      "import": {
        "types": "./dist/membership/index.d.ts",
        "default": "./dist/membership/index.js"
      },
      "require": {
        "types": "./dist/membership/index.d.cts",
        "default": "./dist/membership/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run generate && npm run build && npm run test",
    "preversion": "npm run test && npm run typecheck",
    "version": "npm run generate && npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "release": "npm version patch",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major",
    "release:beta": "npm version prerelease --preid=beta",
    "release:next": "npm version prerelease --preid=next",
    "publish:npm": "npm publish --access public --provenance",
    "publish:beta": "npm publish --access public --tag beta --provenance",
    "publish:next": "npm publish --access public --tag next --provenance"
  },
  "keywords": [
    "iso-3166",
    "iso-3166-1",
    "iso-3166-2",
    "iso-4217",
    "country",
    "countries",
    "subdivision",
    "state",
    "province",
    "region",
    "alpha-2",
    "alpha-3",
    "country-code",
    "postal-code",
    "postcode",
    "zip-code",
    "zipcode",
    "pin-code",
    "pincode",
    "address-validation",
    "currency",
    "dial-code",
    "phone-code",
    "calling-code",
    "continent",
    "geography",
    "eu",
    "sepa",
    "eurozone"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CopperxHQ/countries.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "libphonenumber-js": "^1.11.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  }
}
